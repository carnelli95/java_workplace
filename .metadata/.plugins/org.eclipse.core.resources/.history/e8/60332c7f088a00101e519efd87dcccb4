package com.ruby.java.ch04배열;

import java.util.Random;

public class MergeArray {
	
	public static void main(String[] args) {
		
		int n = 5;
		Random rd = new Random();
		int[] A = new int[5];
		int[] B = new int[10];
		int[] C = new int[10];	
		
		for(int i = 0; i < A.length; i++) {
			A[i] = rd.nextInt(10);
			B[i] = rd.nextInt(10);
		}

		for(int i = 0; i < A.length; i++) {
			System.out.print(A[i] + ", ");
			System.out.print(B[i] + ", ");
		}
		
		for(int i = 0; i < A.length; i++) {
			C[i] = A[i] + B[i];
		}
	}
}
