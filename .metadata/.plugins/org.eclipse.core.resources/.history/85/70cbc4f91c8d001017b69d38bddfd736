package com.ruby.java.ch06객체구현;

class Armor {
	private String name;
	private int height;

	public String getName() { // getter
		return name;
	}

	public void setName(String value) { // setter
		name = value;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int value) {
		height = value;
	}
}